name: dockercompose4804198134870143429
services:
  be_account:
    build:
      context: D:\Project_C#_API\be_account
      dockerfile: be_account/Dockerfile
    depends_on:
      be_account.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ConnectionStrings__DefaultConnection: '"Host=be_account.db;Database=ManageAccount;Username=postgres;Password=123456"'
    image: beaccount
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 8081
        published: "5001"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Quan\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Quan\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  be_account.db:
    environment:
      POSTGRES_DB: '"ManageAccount"'
      POSTGRES_PASSWORD: '"123456"'
      POSTGRES_USER: '"postgres"'
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: bind
        source: D:\Project_C#_API\be_account\volumes\data\db
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose4804198134870143429_default